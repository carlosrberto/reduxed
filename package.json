{
  "name": "reduxed",
  "version": "0.1.0",
  "description": "Reduxed - Reduced Redux Boilerplate",
  "main": "lib/index.js",
  "repository": "git@github.com:carlosrberto/reduxed.git",
  "author": "Carlos Roberto Gomes Junior <carlos.rberto@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "static-server": "^2.2.0",
    "version-bump-prompt": "^4.0.0"
  },
  "scripts": {
    "start": "npm run test -- --watch",
    "test": "jest",
    "cov": "jest --coverage",
    "cov:serve": "npm run cov && static-server .coverage/lcov-report --port 3000 --no-nocache",
    "cov:publish": "cat ./.coverage/lcov.info | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf lib",
    "build": "rollup -c",
    "bump": "bump --prompt --commit --tag --push"
  }
}
